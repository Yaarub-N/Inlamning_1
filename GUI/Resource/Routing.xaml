<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewmodel="clr-namespace:GUI.ViewModels"
                    xmlns:views="clr-namespace:GUI.Views">

    <DataTemplate DataType="{x:Type viewmodel:OverViewModel}">
        <views:OverView/>

    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodel:CreateViewModel}">
        <views:CreateView/>

    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodel:EditViewModel}">
        <views:EditView/>

    </DataTemplate>



</ResourceDictionary>